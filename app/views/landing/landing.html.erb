<h1>Landing#index</h1>
<p>Find me in app/views/landing/index.html.erb</p>

<a href= "/picture/new" alt="#"><h2>Upload a picture</h2></a>

<div class="grid">

                 <% Picture.all.each do |pic| %>
                 <figure class="effect-lexi">
                 <%= image_tag pic.picture, height: '293', width: '480' %>
                   <figcaption>
                     <h2><%= link_to pic.title, pic%></h2>
                     <p><%= pic.name %><br></p>
                     <!-- <p><%= link_to pic.formatted_price, pic %></p> -->

                   </figcaption>
                 </figure>
                 <%end%>

               </div>
